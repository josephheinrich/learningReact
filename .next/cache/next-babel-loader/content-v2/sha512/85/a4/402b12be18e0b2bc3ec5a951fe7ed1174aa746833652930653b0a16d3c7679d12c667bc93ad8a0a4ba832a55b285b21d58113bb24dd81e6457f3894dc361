var keys = Object.keys(cryptoData);\r\n    var rows = [];\r\n\r\n    for (var i = 0; i < keys.length; i++) { \r\n        var key = (keys[i]);\r\n        rows.push(cryptoData[key]);\r\n    }\r\n\r\n    return props.children({\r\n        rows: rows\r\n    });\r\n}\r\n\r\nexport default CryptoRenderProps;"]},"metadata":{},"sourceType":"module"}